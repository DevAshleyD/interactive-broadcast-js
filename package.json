{
  "name": "IB",
  "appName": "Interactive Broadcast Solution",
  "engines": {
    "node": "4.4.2"
  },
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest; npm run e2e-chrome; npm run e2e-firefox; npm run e2e-ie;",
    "build-dev": "browserify -e app/bootstrap.js -t reactify -o public/javascripts/bundle.dev.js",
    "build-min": "browserify -e app/bootstrap.js -t reactify -t uglifyify -o public/javascripts/bundle.min.js",
    "build-tdd": "browserify -e test/spec/suite.js -t reactify -t rewireify -o test/spec/bundle.js",
    "postinstall": "echo postinstall time; ./node_modules/grunt-cli/bin/grunt build",
    "e2e-ie": "node node_modules/protractor/bin/protractor tests/protractor-ie.conf.js --verbose",
    "e2e-chrome": "node node_modules/protractor/bin/protractor tests/protractor-chrome.conf.js --verbose",
    "e2e-firefox": "node node_modules/protractor/bin/protractor tests/protractor-ff.conf.js --verbose"
  },
  "dependencies": {
    "aws-sdk": "^2.1.27",
    "bluebird": "^3.3.4",
    "body-parser": "~1.12.0",
    "browserify": "^10.1.3",
    "classnames": "^2.2.3",
    "compression": "^1.5.2",
    "connect-redis": "^3.0.2",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "ejs": "^2.3.1",
    "express": "~4.12.2",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.11.2",
    "express-useragent": "^0.2.4",
    "firefox-profile": "^0.3.11",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.8.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-react": "^0.12.2",
    "handlebars": "^3.0.3",
    "heroku-ssl-redirect": "0.0.3",
    "hls.js": "^0.5.32",
    "https": "^1.0.0",
    "jest-cli": "^0.4.1",
    "jit-grunt": "^0.9.1",
    "jquery": "^2.1.4",
    "jquery.modal": "^0.2.4",
    "less": "^2.5.0",
    "load-grunt-tasks": "^3.1.0",
    "moment": "2.10.2",
    "morgan": "~1.5.1",
    "nconf": "^0.7.1",
    "nconf-strip-json-comments": "^0.1.0",
    "newrelic": "^1.25.5",
    "node-jsx": "^0.13.3",
    "nodemailer": "^2.5.0",
    "nonce": "^1.0.3",
    "opentok-solutions-logging": "^1.0.5",
    "protractor": "^2.1.0",
    "react": "^0.13.2",
    "react-bootstrap-daterangepicker": "^0.2.5",
    "react-components": "^0.2.0",
    "react-dnd": "^1.0.0",
    "react-router": "^0.13.3",
    "react-s3-uploader": "^2.0.0",
    "react-sortable": "0.0.2",
    "react-toastr": "^1.4.0",
    "react-tools": "^0.13.2",
    "react-window-mixins": "0.0.6",
    "react-zeroclipboard": "^1.1.0",
    "reactify": "~0.14.0",
    "redis": "^2.6.2",
    "request": "^2.55.0",
    "secrets.js": "^0.1.8",
    "serve-favicon": "~2.2.0",
    "socket.io": "^1.3.5",
    "uglify-js": "2.4.21",
    "uglifyify": "^3.0.1",
    "underscore": "^1.8.3",
    "underscore.nested": "^0.1.0",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-node-inspector": "^0.4.2",
    "grunt-nodemon": "^0.4.0",
    "jest": "^0.1.39",
    "nodemon": "^1.2.1",
    "rewireify": "0.0.13",
    "watchify": "^3.1.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/support/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
